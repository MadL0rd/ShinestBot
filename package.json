{
  "name": "shinest-bot",
  "version": "0.2.5",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"scripts/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "scripts:run": "nest start --config scripts/nest-cli.json --path scripts/tsconfig.json -- gum",
    "scripts:build": "nest build --config scripts/nest-cli.json --path scripts/tsconfig.json",
    "scripts:debug": " nest start --debug --watch  --config scripts/nest-cli.json --path scripts/tsconfig.json -- gum",
    "scripts:execute-gum": "(path-exists scripts/dist/scripts || npm run scripts:build) && node scripts/dist/scripts/commands-main gum",
    "gen": "npx shinest generate",
    "check-spell": "cspell ."
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.750.0",
    "@google-cloud/local-auth": "^3.0.1",
    "@nestjs/common": "^11.0.6",
    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "^11.0.6",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/mongoose": "^11.0.0",
    "@nestjs/platform-express": "^11.0.6",
    "@nestjs/schedule": "^6.0.0",
    "async-mutex": "^0.5.0",
    "chalk": "^4.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dotenv": "^16.5.0",
    "drizzle-orm": "^0.44.2",
    "express": "^4.21.2",
    "googleapis": "^144.0.0",
    "moment": "^2.30.1",
    "moment-timezone": "^0.6.0",
    "mongoose": "^8.9.5",
    "nest-winston": "^1.10.2",
    "nestjs-telegraf": "^2.8.1",
    "node-jose": "^2.2.0",
    "pg": "^8.15.6",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "telegraf": "^4.16.3",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^5.0.0",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.3/xlsx-0.20.3.tgz",
    "yandex-speech-promise": "^1.3.3",
    "zod": "^4.0.0"
  },
  "devDependencies": {
    "@clack/prompts": "^0.9.1",
    "@eslint/js": "^9.23.0",
    "@madl0rd/shinest-cli": "^0.2.7",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/express": "^5.0.0",
    "@types/jest": "29.5.14",
    "@types/node": "^22.13.5",
    "@types/node-fetch": "^2.6.12",
    "@types/node-jose": "^1.1.13",
    "@types/pg": "^8.11.14",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "drizzle-kit": "^0.31.0",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "nest-commander": "^3.16.0",
    "path-exists-cli": "2.0.0",
    "prettier": "^3.5.3",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.3",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.28.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
